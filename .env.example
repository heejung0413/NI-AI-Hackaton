# AWS 자격 증명
REACT_APP_AWS_ACCESS_KEY_ID=your-access-key-id
REACT_APP_AWS_SECRET_ACCESS_KEY=your-secret-access-key

# S3 버킷 이름
REACT_APP_S3_BUCKET_NAME=your-s3-bucket-name

# AWS 리전 (선택사항, 기본값: us-east-1)
REACT_APP_AWS_REGION=us-east-1

# API 서버 URL (백엔드가 실행되는 서버의 주소)
VITE_API_URL=http://localhost:3001

# 프로덕션 환경에서는 다음과 같이 설정:
# VITE_API_URL=http://ec2-13-125-174-173.ap-northeast-2.compute.amazonaws.com:3001