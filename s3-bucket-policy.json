{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "AllowUserUpload",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::455020520696:user/ni-ai-hackton"
            },
            "Action": [
                "s3:PutObject",
                "s3:PutObjectAcl",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::ni-ai-hackaton/*"
        },
        {
            "Sid": "AllowUserListBucket",
            "Effect": "Allow",
            "Principal": {
                "AWS": "arn:aws:iam::455020520696:user/ni-ai-hackton"
            },
            "Action": [
                "s3:ListBucket",
                "s3:GetBucketLocation"
            ],
            "Resource": "arn:aws:s3:::ni-ai-hackaton"
        }
    ]
}